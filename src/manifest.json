{
  "manifest_version": 2,
  "name": "SoundCloud set filter",
  "version": "0.1",
  "permissions": [
    "*://soundcloud.com/",
    "*://api-v2.soundcloud.com/stream"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://soundcloud.com/stream"
      ],
      "js": ["jquery-3.4.1.min.js", "content.js"],
      "run_at": "document_start"
    }
  ]
}
