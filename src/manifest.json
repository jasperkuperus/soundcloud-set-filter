{
  "manifest_version": 2,
  "name": "SoundCloud set filter",
  "version": "0.2",
  "permissions": [
    "*://soundcloud.com/",
    "*://api-v2.soundcloud.com/stream"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://soundcloud.com/stream"
      ],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
   "128": "icon128.png"
  }
}
